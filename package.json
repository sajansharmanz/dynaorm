{
  "name": "dynaorm",
  "version": "1.0.10",
  "description": "A type-safe DynamoDB ORM",
  "keywords": [
    "AWS",
    "DynamoDB",
    "ORM",
    "TypeScript"
  ],
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Sajan Sharma",
    "email": "sajansharmanz@gmail.com",
    "url": "https://sajansharma.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sajansharmanz/dynaorm.git"
  },
  "bugs": {
    "url": "https://github.com/sajansharmanz/dynaorm/issues"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "prettier --write .",
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc --watch",
    "test": "vitest --watch=false",
    "coverage": "vitest run --coverage --watch=false"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": ">=3.886.0",
    "@aws-sdk/util-dynamodb": ">=3.886.0",
    "p-throttle": ">=8.0.0",
    "zod": ">=4.1.5"
  }
}
